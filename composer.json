{
  "name": "kazitoha/laravel-activity-logger",
  "description": "Activity logging for Laravel models via a simple trait. Logs create/update/delete with old/new values.",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "activity", "audit", "log", "logger"],
  "authors": [
    { "name": "kazitoha" }
  ],
  "autoload": {
    "psr-4": {
      "Kazitoha\\ActivityLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kazitoha\\ActivityLogger\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kazitoha\\ActivityLogger\\ActivityLoggerServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/database": "^10.0|^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "pestphp/pest": "^3.0",
    "phpunit/phpunit": "^10.5"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
